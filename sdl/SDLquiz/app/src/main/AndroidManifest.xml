<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sdlquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".quizlistActivity"/>
        <activity android:name=".sdl_insertActivity"/>
        <activity android:name=".sdlMainMenuActivity"/>
        <activity android:name=".quizlist_modify_Activity"/>
        <activity android:name=".takequizActivity"/>
        <activity android:name=".quizResultActivity"/>
        <activity android:name=".quizWrongActivity" />
    </application>

</manifest>